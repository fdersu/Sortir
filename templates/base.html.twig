<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %} Welcome! {% endblock %}</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-+0n0xVW2eSR5OomGNYDnhzAbDsOXxcvSN1TPprVMTNDbiYZCxYbOOl7+AMvyTG2x" crossorigin="anonymous">
    <link rel="icon" href="{{ asset('img/Logo-sortie.png') }}">
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('styles/style.css') }}">
    {% endblock %}

    {% block javascripts %}
        {# {{ encore_entry_script_tags('app') }} #}
    {% endblock %}
</head>
<body>

<header>
    <h1><img src="{{ asset('img/Logo-sortie.png') }}" height="50px" alt="Sortie !"></h1>

    {% if app.user %}
        {% include 'menu/nav.html.twig' %}
    {% endif %}
</header>
{% for label, messages in app.flashes %}
    {% for message in messages %}
        <center>
            <div class="alert alert-primary flash flash-{{ label }}">
                {{ message }}
            </div>
        </center>
    {% endfor %}
{% endfor %}
{% block body %}{% endblock %}
{% block style %}{% endblock %}

</body>

<script src="{{ asset('js/jquery-3.6.0.js') }}"></script>
<script src="{{ asset('js/addSortie.js') }}"></script>
</html>
